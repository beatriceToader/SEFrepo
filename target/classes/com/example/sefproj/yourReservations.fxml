                <Font name="Serif Italic" size="12.0" />
            </font>
        </Button>
        <Label fx:id="reservationsDetailsMessage" alignment="TOP_LEFT" layoutX="44.0" layoutY="91.0" prefHeight="262.0" prefWidth="513.0" textFill="WHITE" />
        <Button fx:id="backButton" layoutX="30.0" layoutY="363.0" mnemonicParsing="false" onAction="#backButtonOnAction" prefHeight="22.0" prefWidth="55.0" style="-fx-background-color: #000000;" text="back" textFill="WHITE">
