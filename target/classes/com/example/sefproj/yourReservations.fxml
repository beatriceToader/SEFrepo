<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #472f17;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sefproj.YourReservationsController">
    <children>
        <Text fx:id="yourReservationsText" fill="WHITE" layoutX="200.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Your own reservations">
