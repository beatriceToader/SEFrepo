                <Font name="Serif Italic" size="22.0" />
            </font>
        </Text>
        <Button fx:id="exitButton" layoutX="566.0" mnemonicParsing="false" onAction="#exitButtonOnAction" prefHeight="22.0" prefWidth="33.0" style="-fx-background-color: #ffffff;" text="exit">
