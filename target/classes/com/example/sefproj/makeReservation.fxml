<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #A00000;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sefproj.MakeReservationController">
    <Button fx:id="exitButton" layoutX="566.0" mnemonicParsing="false" onAction="#exitButtonOnAction" prefHeight="22.0" prefWidth="33.0" style="-fx-background-color: #ffffff;" text="exit">
        <font>
            <Font name="Serif Italic" size="12.0" />
        </font>
    </Button>
    <Button fx:id="backButton" layoutX="63.0" layoutY="337.0" mnemonicParsing="false" onAction="#backButtonOnAction" prefHeight="23.0" prefWidth="60.0" style="-fx-background-color: #000000;" text="back" textFill="WHITE">
        <font>
            <Font name="Serif Italic" size="12.0" />
        </font>
    </Button>
    <Text fx:id="makeReservationText" fill="WHITE" layoutX="214.0" layoutY="77.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Make a reservation">
        <font>
            <Font name="Serif Italic" size="23.0" />
        </font>
    </Text>
    <Label layoutX="115.0" layoutY="166.0" text="concert name:" textFill="WHITE">
        <font>
            <Font name="Serif Italic" size="17.0" />
        </font>
    </Label>
    <TextField fx:id="concertNameTextField" layoutX="220.0" layoutY="163.0" prefHeight="25.0" prefWidth="234.0" />
    <Label fx:id="reservationSuccessfullyMessage" alignment="CENTER" layoutX="100.0" layoutY="243.0" prefHeight="25.0" prefWidth="400.0" textFill="WHITE">
        <font>
            <Font name="Serif Italic" size="17.0" />
        </font>
    </Label>
   <Button fx:id="saveButton" layoutX="478.0" layoutY="337.0" mnemonicParsing="false" onAction="#saveButtonOnAction" prefHeight="23.0" prefWidth="60.0" style="-fx-background-color: #000000;" text="save" textFill="WHITE">
      <font>
         <Font name="Serif Italic" size="12.0" />
      </font>
   </Button>
</AnchorPane>
